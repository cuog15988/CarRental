
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}
  

<section class="banner-area relative" id="home">
    <div class="overlay overlay-bg"></div>
    <div class="container">
        <div class="row fullscreen d-flex align-items-center justify-content-center">
            <div class="banner-content col-lg-7 col-md-6 ">
                <h6 class="text-white ">the Royal Essence of Journey</h6>
                <h1 class="text-white text-uppercase">
                    Thuê xe - Tìm kiếm, So sánh và Tiết kiệm
                </h1>
                <p class="pt-20 pb-20 text-white">
                    So sánh 900 công ty tại hơn 60,000 địa điểm Bảo đảm giá thấp nhất
                </p>
                <a href="#" class="primary-btn text-uppercase">Xem Thêm</a>
            </div>
            <div class="col-lg-5  col-md-6 header-right">
                <h4 class="text-white pb-30">TÌM XE CHO THUÊ</h4>
                <div class="form" role="form" autocomplete="off">
                    <div class="form-group">
                        <div class="default-select">
                            <select>
                                <option value="" disabled selected hidden>Chọn Loại Xe</option>
                                <option value="1">BMW</option>
                                <option value="1">Farrari</option>
                                <option value="1">Toyota</option>
                            </select>
                        </div>

                    </div>

                    <div class="form-group">
                        <div class="default-select">
                            <select>
                                <option value="" disabled selected hidden>Tỉnh</option>
                                <option value="1">BMW</option>
                                <option value="1">Farrari</option>
                                <option value="1">Toyota</option>
                            </select>
                        </div>

                    </div>
                    <div class="form-group">
                        <div class="default-select" id="default-select">
                            <select>
                                <option value="" disabled selected hidden>Vị Trí</option>
                                <option value="1">BMW</option>
                                <option value="1">Farrari</option>
                                <option value="1">Toyota</option>
                            </select>
                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-md-6 wrap-left">
                            <div class="input-group dates-wrap">
                                <input id="datepicker" class="dates form-control" id="exampleAmount" placeholder="Ngày nhận xe" type="text">
                                <div class="input-group-prepend">
                                    <span class="input-group-text"><span class="lnr lnr-calendar-full"></span></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 wrap-right">
                            <div class="form-group row">
                                <div class="col-md-6 wrap-left">

                                    <input type="text" id="time" data-format="HH:mm" data-template="HH : mm" name="datetime">

                                </div>
                                <div class="col-md-6 wrap-left">
                                    <select name="puMinute" id="puMinute" class="time-panel__min-select minute"><option value="0" selected="selected">00</option><option value="15">15</option><option value="30">30</option><option value="45">45</option></select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-md-6 wrap-left">
                            <div class="input-group dates-wrap">
                                <input id="datepicker2" class="dates form-control" id="exampleAmount" placeholder="Ngày nhận xe" type="text">
                                <div class="input-group-prepend">
                                    <span class="input-group-text"><span class="lnr lnr-calendar-full"></span></span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 wrap-right">
                            <div class="form-group row">
                                <div class="col-md-6 wrap-left">
                                    <input type="text" id="datetime24" data-format="DD-MM-YYYY HH:mm" data-template="DD / MM / YYYY     HH : mm" name="datetime" value="21-12-2012 20:30">
                                </div>
                                <div class="col-md-6 wrap-left">
                                    <select name="puMinute" id="puMinute" class="time-panel__min-select minute"><option value="0" selected="selected">00</option><option value="15">15</option><option value="30">30</option><option value="45">45</option></select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="from-group">
                        <input class="form-control txt-field" type="text" name="name" placeholder="Your name">

                    </div>
                    <div class="form-group row">
                        <div class="col-md-12">
                            <button type="reset" class="btn btn-default btn-lg btn-block text-center text-uppercase">Confirm Car Booking</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
