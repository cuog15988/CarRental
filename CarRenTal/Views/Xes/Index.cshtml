@model IEnumerable<CarRenTal.Models.Xe>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NgayNhap)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Huyen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaNguoiDangNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaTenXeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NgayNhap)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Huyen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaNguoiDangNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaTenXeNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
