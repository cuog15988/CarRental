@model CarRenTal.Models.Xe

@{
    ViewData["Title"] = "Book this car";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<div class="row align-items single-model item" style="padding-top:80px">

    <div class="col-lg-3 model-left">
    </div>
    <div class="col-lg-7 model-right" style="">
        <div class="card">

            <div class="card-body">
                <div class="e-sample-resize-container" style="">
                    <div style="">

                        <div id="Thongtin" class="container">
                            @foreach (var item in ViewBag.xe1)
                            {

                                <div class="row align-items-center single-model item">
                                    <div class="col-lg-4 model-left" style="border-right:solid; border-color:gray;border-width:1px">
                                        <h6 class="mt-20" style="margin-top:0rem !important;margin-bottom:0rem">@item.Tenxe </h6>
                                        <p style="color:#808080">This car is in high demand! </p>

                                        <img id="img-Sanpham" class="img-fluid" src="@item.Hinh" alt="" style="min-width:100%; height:166px" />

                                        <div class="" style="text-align:right;text-decoration: underline; font-size:12px">

                                            <a href="/DichVu/Detail?id=@item.Id" class="">car details</a>
                                        </div>
                                    </div>
                                    <div class="col-lg-7 model-right">
                                        <a style="float:right;font-size:12px;color:#489ae3 ;text-decoration:underline;cursor:pointer" data-toggle="modal" data-target="#exampleModalCenter">change dates</a>

                                        <p style="font-weight:bold; color:#0f6193;margin-top:0rem !important;margin-bottom:0rem">Pick-up </p>  @String.Format("{0:dddd, MMMM d, yyyy}", CarRenTal.wwwroot.DAO.Seacrch.ngaynhan)
                                        <p style="font-weight:600">@item.MaNguoiDangNavigation.DiaChi</p>

                                        <hr />
                                        <h6 style="font-weight:bold; color:#0f6193;margin-top:0rem !important;margin-bottom:0rem">Drop-off</h6>

                                        <p style="" id="abc">@String.Format("{0:dddd, MMMM d, yyyy}", CarRenTal.wwwroot.DAO.Seacrch.ngaytra)</p>
                                        <p style="font-weight:600">@item.MaNguoiDangNavigation.DiaChi</p>
                                        <hr />

                                    </div>
                                </div>
                                @Html.Partial("date")
                            }
                        </div>

                    </div>
                </div>
            </div>
        </div>


    </div>
</div>
<div class="row align-items single-model item" style="padding-top:20px ; margin-bottom:40px">

    <div class="col-lg-3 model-left">
    </div>
    <div class="col-lg-7 model-right" style="">
        @await Component.InvokeAsync("student", new { id = CarRenTal.DAO.Common.CommonConstants.UserID, maxe = ViewBag.Id })
    </div>
</div>

<div class="row align-items single-model item" style="padding-top:20px ; margin-bottom:40px">

    <div class="col-lg-3 model-left">
    </div>
    <div class="col-lg-7 model-right" style="">
        <div class="card">
            <div class="card-body">

                <p class="card-title" style="font-size:18px !important; font-weight:800">Sẵn sàng cho một số ý tưởng tiết kiệm tiền?</p>
                <p class="card-text" style="font-size:16px">Chúng tôi có thể gửi cho bạn giảm giá và các ưu đãi cho thuê xe khác, giúp bạn tiết kiệm nhiều tiền hơn trong tương lai</p>
                <p style="    display: block;margin-top: 8px;font-size: 13px;line-height: 17px;">liên hệ với chúng tôi qua email!</p>
                <a href="@Url.Action("Index","HomThuLienHes")" target="_blank" id="notme">subscribe</a>
            </div>
        </div>
    </div>
</div>
<div class="row align-items single-model item" style="padding-top:20px ; margin-bottom:40px">

    <div class="col-lg-3 model-left">
    </div>
    <div class="col-lg-7 model-right" style="">
        <a href="@Url.Action("getBook","Book", new { id=ViewBag.Id})">
        <input type="button" id="btn" class="btn btn-primary" style="float:right; background-color:rgb(0, 135, 77);width:250px;height :60px" value="BOOK NOW"/>
        </a>
    </div>
</div>







@section Scripts {

    <script>
        var e = document.getElementById("datepicker2");
        var text = e.options[e.selectedIndex].text;
    </script>

}



